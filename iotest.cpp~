#include <iostream>
#include <stdlib.h>
using namespace std;

int main(int argc, const char * argv[])
{
    int x; double y;
    cout<< "enter an integer and a double:"<< endl;
    cin>>x;
    cin>>y;
    cout<< "the integer you entered is:"<< x << endl;
    cout<< "the double you entered is:" << y << endl;
    return 0;

}

